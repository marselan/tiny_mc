#!/usr/bin/gnuplot
set terminal png size 500,500
set output 'photons_per_seconds.png'
set xlabel 'flags configuration'
set ylabel 'Photons per seconds'
unset key
set xrange []
set yrange [0:850]
set style fill solid
set boxwidth 0.5
plot 'averages.dat' using 2:xticlabels(1)  with boxes,\
'averages.dat' using 0:($2+15):(sprintf("%3.2f",$2)) with labels notitle
