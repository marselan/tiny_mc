#!/usr/bin/gnuplot
set terminal png size 500,500
set output 'cache_misses.png'
set xlabel 'flags configuration'
set ylabel 'Cache-misses [%] of all cache refs'
unset key
set xrange []
set yrange [0:40]
set style fill solid
set boxwidth 0.5
plot 'averages.dat' using 5:xticlabels(1)  with boxes,\
'averages.dat' using 0:($5+2):(sprintf("%3.2f",$5)) with labels notitle
